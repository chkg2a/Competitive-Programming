digraph {
	"opt(3,6)" [label="opt(3,6)"]
	"opt(2,6,[1, 2, 5],[2, 3, 4])" [label="opt(2,6,[1, 2, 5],[2, 3, 4])"]
	"opt(3,6)" -> "opt(2,6,[1, 2, 5],[2, 3, 4])"
	"opt(1,6,[1, 2, 5],[2, 3, 4])" [label="opt(1,6,[1, 2, 5],[2, 3, 4])"]
	"opt(2,6,[1, 2, 5],[2, 3, 4])" -> "opt(1,6,[1, 2, 5],[2, 3, 4])"
	"opt(0,6,[1, 2, 5],[2, 3, 4])" [label="opt(0,6,[1, 2, 5],[2, 3, 4])"]
	"opt(1,6,[1, 2, 5],[2, 3, 4])" -> "opt(0,6,[1, 2, 5],[2, 3, 4])"
	"1 + opt(0,4,[1, 2, 5],[2, 3, 4])" [label="1 + opt(0,4,[1, 2, 5],[2, 3, 4])"]
	"opt(1,6,[1, 2, 5],[2, 3, 4])" -> "1 + opt(0,4,[1, 2, 5],[2, 3, 4])"
	"2 + opt(1,3,[1, 2, 5],[2, 3, 4])" [label="2 + opt(1,3,[1, 2, 5],[2, 3, 4])"]
	"opt(2,6,[1, 2, 5],[2, 3, 4])" -> "2 + opt(1,3,[1, 2, 5],[2, 3, 4])"
	"opt(0,3,[1, 2, 5],[2, 3, 4])" [label="opt(0,3,[1, 2, 5],[2, 3, 4])"]
	"2 + opt(1,3,[1, 2, 5],[2, 3, 4])" -> "opt(0,3,[1, 2, 5],[2, 3, 4])"
	"1 + opt(0,1,[1, 2, 5],[2, 3, 4])" [label="1 + opt(0,1,[1, 2, 5],[2, 3, 4])"]
	"2 + opt(1,3,[1, 2, 5],[2, 3, 4])" -> "1 + opt(0,1,[1, 2, 5],[2, 3, 4])"
	"5 + opt(2,2,[1, 2, 5],[2, 3, 4])" [label="5 + opt(2,2,[1, 2, 5],[2, 3, 4])"]
	"opt(3,6)" -> "5 + opt(2,2,[1, 2, 5],[2, 3, 4])"
	"opt(1,2,[1, 2, 5],[2, 3, 4])" [label="opt(1,2,[1, 2, 5],[2, 3, 4])"]
	"5 + opt(2,2,[1, 2, 5],[2, 3, 4])" -> "opt(1,2,[1, 2, 5],[2, 3, 4])"
	"opt(0,2,[1, 2, 5],[2, 3, 4])" [label="opt(0,2,[1, 2, 5],[2, 3, 4])"]
	"opt(1,2,[1, 2, 5],[2, 3, 4])" -> "opt(0,2,[1, 2, 5],[2, 3, 4])"
	"1 + opt(0,0,[1, 2, 5],[2, 3, 4])" [label="1 + opt(0,0,[1, 2, 5],[2, 3, 4])"]
	"opt(1,2,[1, 2, 5],[2, 3, 4])" -> "1 + opt(0,0,[1, 2, 5],[2, 3, 4])"
	"2 + opt(1,-1,[1, 2, 5],[2, 3, 4])" [label="2 + opt(1,-1,[1, 2, 5],[2, 3, 4])"]
	"5 + opt(2,2,[1, 2, 5],[2, 3, 4])" -> "2 + opt(1,-1,[1, 2, 5],[2, 3, 4])"
}
